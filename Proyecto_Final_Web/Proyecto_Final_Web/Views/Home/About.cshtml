@{
    ViewBag.Title = "About";
}
<h2 class="text-info">Usuarios del sistema</h2>



<table class="table border-1 border-info table-info">
    <tr class="bg-info">
        <th>
            ID
        </th>
        <th>
            EMAIL
        </th>
        <th>
            USERNAME
        </th>
    </tr>

    @foreach (var item in ViewData["Usuarios"] as IEnumerable<Proyecto_Final_Web.Models.Listas>)
    {
    <tr>
        <td>@item.listaUsuarios.Id</td>
        <td>@item.listaUsuarios.Email </td>

        <td>@item.listaUsuarios.UserName</td>


    </tr>


    }
 
</table>

